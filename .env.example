MONGODB_PASSWORD= pass
MONGODB_HOSTNAME= mongodb
PYTHONUNBUFFERED= 1

#MONGO -> mongo.env
MONGO_INITDB_ROOT_USERNAME= root
MONGO_INITDB_ROOT_PASSWORD= pass
MONGO_INITDB_DATABASE= new_db