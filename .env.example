MAINTAINER=FirstName LastName <example@domain.com>

#MONGO EXPRESS 
ME_CONFIG_MONGODB_ADMINUSERNAME= root
ME_CONFIG_MONGODB_ADMINPASSWORD= example
ME_CONFIG_MONGODB_URL= mongodb://root:pass@mongodb:27017/

#NGINX -> no need since no sensitive data
APP_ENV= "dev"
APP_NAME= "webserver"
APP_DEBUG= "true"
SERVICE_NAME= "webserver"

#FLASK -> flask.env
REDIS_URL= redis
REDIS_PORT= 6379
MONGODB_HOST= mongodb
